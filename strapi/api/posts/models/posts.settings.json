{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "posts"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "html": {
      "type": "richtext"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "tutorial",
        "paper",
        "misc"
      ]
    },
    "slug": {
      "type": "string"
    },
    "date": {
      "type": "date"
    }
  }
}
